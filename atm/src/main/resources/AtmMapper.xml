<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//OTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="bob.homework.atm.mapper.AtmMapper">
    <select id="getAllAtm">
        select * from atm
    </select>
    <update id="updateAtm">
        update atm set status = #{status}, bankcardId = #{bankCardId} WHERE id = #{id}
    </update>
</mapper>